{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-plusplus": "off",
    "max-len": [
      "error",
      150,
      {
        "ignoreUrls": true
      }
    ],
    "no-tabs": "off",
    "no-console": "off",
    "linebreak-style": "off",
    "indent": "off",
    "no-trailing-spaces": "off"
  }
}